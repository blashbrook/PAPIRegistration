{
    "name": "blashbrook/papiregistration",
    "description": "A custom registration form for the Polaris ILS using the Polaris API",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Lashbrook",
            "email": "blashbrook@gmail.com",
            "homepage": "https://github.com/blashbrook"
        }
    ],
    "homepage": "https://github.com/blashbrook/papiregistration",
    "keywords": ["Laravel", "PAPIRegistration"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Blashbrook\\PAPIRegistration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blashbrook\\PAPIRegistration\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Blashbrook\\PAPIRegistration\\PAPIRegistrationServiceProvider"
            ],
            "aliases": {
                "PAPIRegistration": "Blashbrook\\PAPIRegistration\\Facades\\PAPIRegistration"
            }
        }
    }
}
